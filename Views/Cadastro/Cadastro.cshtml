@{
    ViewData["Title"] = "Cadastro";
    int pageSize = 10;
    int currentPage = (Request["page"] == null) ? 1 : int.Parse(Request["page"]);
    int totalItems = Model.Count();
    int totalPages = (int)Math.Ceiling(totalItems / (double)pageSize);
    IEnumerable<T> page = Model.Skip((currentPage - 1) * pageSize).Take(pageSize);
}

<div>
    <table class="table">
        <thead>
            <tr>
                <th>Item</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in page)
            {
                <tr>
                    <td>@item</td>
                </tr>
            }
        </tbody>
    </table>

    <nav aria-label="Page navigation example">
        <ul class="pagination">
            <li class="page-item @(currentPage == 1 ? "disabled" : "")">
                <a class="page-link" href="#" onclick="loadPage(@(currentPage - 1))">Previous</a>
            </li>
            @for (int i = 1; i <= totalPages; i++)
            {
                <li class="page-item @(i == currentPage ? "active" : "")">
                    <a class="page-link" href="#" onclick="loadPage(@i)">@i</a>
                </li>
            }
            <li class="page-item @(currentPage == totalPages ? "disabled" : "")">
                <a class="page-link" href="#" onclick="loadPage(@(currentPage + 1))">Next</a>
            </li>
        </ul>
    </nav>
</div>